    "db:studio": "prisma studio",
    "db:studio:dev": "env-cmd -f .env.development.local prisma studio",
    "db:seed": "tsx scripts/seed.ts",
    "db:rls:disable": "node scripts/manage-rls.js disable",
    "db:rls:enable": "node scripts/manage-rls.js enable"
  },
