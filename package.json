  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "setup": "node scripts/check-setup.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
